@model Store
<div class="card mb-4">
    <div class="card-body">
        <div class="row">
            <div class="col-lg-6">
                <h3 class="h6">Store info</h3>
                <address>
                    <strong>Seller naame : @Model.Name</strong><br>
                    Governace : @Model.Governace<br>
                    City : @Model.City<br>
                    Description : @Model.Street<br>
                </address>
            </div>
        </div>
    </div>
</div>
